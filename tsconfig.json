{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@assets": ["src/assets"],
      "@components": ["src/components"],
      "@constants": ["src/constants"],
      "@routes": ["src/routes"],
      "@screens": ["src/screens"],
      "@utils": ["src/utils"]
    }
  }
}
